{
  "firestore": {
    "rules": "firestore.rules",
    "indexes": "firestore.indexes.json"
  },
  "hosting": {
    "public": "build",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "appAssociation": "AUTO", 
    "rewrites": [ {
      "source": "confluence-io.web.app/confluence/**",  
      "dynamicLinks": true
    }]
  }
}
